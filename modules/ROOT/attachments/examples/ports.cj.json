{
  "$schema": "https://j-s-o-n.org/schema/cj-7.0.0.json",
  "graphs": [
    {
      "id": "g_ports",
      "label": { "entries": [ { "value": "Circuit Diagram with Ports" } ] },
      "nodes": [
        {
          "id": "and_gate",
          "label": { "entries": [ { "value": "AND Gate" } ] },
          "ports": [
            { "id": "in1" },
            { "id": "in2" },
            { "id": "out" }
          ]
        },
        {
          "id": "ic1",
          "label": { "entries": [ { "value": "Microcontroller" } ] },
          "ports": [
            {
              "id": "gpio",
              "label": { "entries": [ { "value": "GPIO Bank" } ] },
              "ports": [
                { "id": "pin1" },
                { "id": "pin2" }
              ]
            }
          ]
        }
      ],
      "edges": [
        {
          "id": "e_logic",
          "endpoints": [
            { "node": "and_gate", "port": "out" },
            { "node": "ic1", "port": "pin1" }
          ]
        }
      ]
    }
  ]
}
